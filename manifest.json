{
  "manifest_version": 3,
  "name": "Request Monitor",
  "version": "1.0",
  "description": "Monitor abnormal network requests",
  "permissions": [
    "webRequest",
    "proxy",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html"
}